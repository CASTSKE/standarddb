<body>
    <FONT FACE="Arial">
            <div class="hb">
              <div class="logo">
                  <a href="/"> <img src="/views/CASTb.png"></a>
                  <span><a href="/index2">Competition</a></span>
                 </div>
                 <form class="example" action="/sonars" method="get" >
                  <center><h2 style="color:black;font-size: 15px;">Search</h2></center>
                  <input type="text"  name="name" id="name" autofocus >
                 <button type="submit" value="submit" ><i class="fa fa-search"></i></button>
              </form> 
              </div>
<section>
<nav>
<a href="/sonarqube">Clear All</a>
      <div class="dropdown">
          <button class="dropbtn">Language  
            <i class="fa fa-caret-down"></i>
          </button>
          <div class="dropdown-content">
            <%for(i=0;i<lang.length;i++){%>
            <a href="/sonars?name=<%=lang[i].langname%>"><%=lang[i].langname%></a>
          <%}%>
          </div>
        </div>
</nav>
<article>
<div style="overflow-x:auto;">
<center><b>Checkmark</b></center>
Count:<%=data.length%><br>
<%if(x){%>
Search result for:    <%=x%>
<%}
%>

<table>
    <tr>
        <!-- <th>Key</th> -->
        <th> Name</th>
        <th>Language</th>
        <th>Tags</th>
    </tr>
    <% for(i=0;i<data.length;i++){ %>
            <tr>
        
             <!-- <td ><%= data[i].key%></a></td> -->
             <td width="60%"><a href="sonarid?name=<%=data[i].key%>"><%= data[i].name%></td>
             <td><%= data[i].langname%></td>
                <td><%=data[i].systags%></td>
            </tr>
        <% } %>
</table>
  
</div>
</article>
</section>
