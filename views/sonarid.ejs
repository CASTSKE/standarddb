<html>
<head>
    <title>Result</title>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Roboto'>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link href="/css/sview.css" type="text/css" rel = "stylesheet">
<link rel="stylesheet" href="themes/prism.css" data-noprefix />
<link rel="stylesheet" href="plugins/line-numbers/prism-line-numbers.css" data-noprefix />
<script src="scripts/prefixfree.min.js"></script>
<style>
  
</style>
</head>
<body class="w3-light-grey">

    <!-- Page Container -->
    <div class="w3-content w3-margin-top" style="max-width:1400px;">
    
      <!-- The Grid -->
      <div class="w3-row-padding">
    
        <!-- Left Column -->
        <div class="w3-third">
    
          <div class="w3-white w3-text-grey w3-card-4">
            <div class="w3-display-container">
              <div class="logo">
               <a href="/"> <img src="/views/CASTb.png"></a>
               <span><a href="/index2">Competition</a>
                <a href="/sonarqube">Sonarqube</a></span>
              
              </div>
    
            </div>
    
            </div>
    
          </div><br>
    
        <!-- End Left Column -->
        </div>
    
<div class="w3-twothird">

    <div class="w3-container w3-card-4 w3-white w3-margin-bottom">
     
      <div class="viewhead">
	  
	  
	  <h5 class="w3-opacity"><p></p></h5>
      <h2 class="w3-text-grey w3-padding-16"></h2>

    </div> 
<div class="w3-container">
     <h5 class="w3-opacity"><b><%= data[0].name%></b></h5>
    
    <hr>
   </div>	
    <div class="w3-container">
        <div id="ele1">
        <h5 class="w3-opacity"><b>Key</b></h5>
        <h6 class="w3-text-teal"><span class="w3-tag w3-teal w3-round"><%= data[0].key%></span></h6>
      </div><div id="ele2">
        <h5 class="w3-opacity"><b>Language</b></h5>
        <p><%= data[0].langname%></p>
      </div><div id="ele3">
      <h5 class="w3-opacity"><b>Severity</b></h5>
      <p><%= data[0].severity%></p>
      </div>
      <div id="ele4">
        <h5 class="w3-opacity"><b>Type</b></h5>
        <p><%= data[0].type%></p>
        </div>
    </div>
      <div class="w3-container"><hr>
        <h5 class="w3-opacity"><b>Tags</b></h5>
        <p><%=data[0].systags %>
      </div>
    </div>
    <div class="w3-container w3-card-4 w3-white w3-margin-bottom">
   <div class="w3-container">
     <h5 class="w3-opacity"><b>Description <i class="fa fa-caret-down"></i></b></h5>
     <div id="myDIV" style="display: none; ">
    </div><p><%- data[0].htmldesc%></p>
    
    <hr>
   </div>
   <div class="w3-container">
     <h5 class="w3-opacity"><b>Parameters</b></h5>
     <p><%=data[0].params%></p>
     
   </div>
</div>
 </div>
  <!-- End Right Column -->
  </div>

<!-- End Grid -->


<!-- End Page Container -->

<footer class="w3-container w3-teal w3-center w3-margin-top">

</footer>

</body>
</html>