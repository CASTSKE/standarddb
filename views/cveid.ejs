<head>
    <title>Result</title>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Roboto'>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link href="/css/sview.css" type="text/css" rel = "stylesheet">

</head>
<body class="w3-light-grey">

    <!-- Page Container -->
    <div class="w3-content w3-margin-top" style="max-width:1400px;">
    
      <!-- The Grid -->
      <div class="w3-row-padding">
    
        <!-- Left Column -->
        <div class="w3-third">
    
          <div class="w3-white w3-text-grey w3-card-4">
            <div class="w3-display-container">
              <div class="logo">
               <a href="/"> <img src="/views/CASTb.png"></a>
			                 
				<span><a href="/index1">Standards</a>
                <a href="/cvesearch">CVE</a></span>

              </div>
    
            </div>
    
            </div>
    
          </div><br>
    
        <!-- End Left Column -->
        </div>
    
<div class="w3-twothird">

    <div class="w3-container w3-card-4 w3-white w3-margin-bottom">
      <div class="viewhead">
      <h2 class="w3-text-grey w3-padding-16">CVE Overview</h2>
     
    <h5 class="w3-text-teal w3-padding-16"><a href="/cvereview?id=<%=kk[0].name%>"><span class="w3-tag w3-teal w3-round">REVIEW</span></a></h5>
    </div>
      <div class="w3-container">
        <div id="ele1">
        <h5 class="w3-opacity"><b>CVE ID</b></h5>
        <h6 class="w3-text-teal"><span class="w3-tag w3-teal w3-round"><%= kk[0].name%></span></h6>
      </div><div id="ele2">
        <h5 class="w3-opacity"><b>Status</b></h5>
        <p><%= kk[0].status%></p>
      </div><div id="ele3">
      <h5 class="w3-opacity"><b>Phase</b></h5>
      <p><%= kk[0].phase%></p>
      </div>
      
      <div class="w3-container"><hr>
        <h5 class="w3-opacity"><b>CWE</b></h5>
        <p><% if(cwe !=null){
            for(i=0;i<cwe.length;i++){%>
            <a href="/cweid?id=<%=cwe[i]%>"><%= cwe[i] %></a>
            <%}}%></p><br>
      </div>
    </div>
</div>
    <div class="w3-container w3-card w3-white">
   <h2 class="w3-text-grey w3-padding-16">Details</h2>
   <div class="w3-container">
     <h5 class="w3-opacity"><b>Description</b></h5>
     <p><%= kk[0].description%></p>
     <hr>
   </div>
   <div class="w3-container">
     <h5 class="w3-opacity"><b>References</b></h5>
     <ul>
       
            <% if(ref!=null){
            for(i=0;i<ref.length;i++){ %> 
                <% if(ref[i].split(":")[1]=='http' || ref[i].split(":")[1]=='https' || ref[i].split(":")[1]=='ftp'){%>
                    <li><a href="<%=ref[i].slice(ref[i].indexOf('http')) %>"><%=ref[i]  %></a> </li>
                <%}  else{ %>
                    <li><%=ref[i] %></li>            
            <% }} }%>
            </ul>
 
    
     <hr>
   </div>
   <div class="w3-container">
     <h5 class="w3-opacity"><b>Votes</b></h5>
     <p><%= kk[0].votest%></p><br>
   </div>
   <div class="w3-container">
    <h5 class="w3-opacity"><b>Comments</b></h5>
    <p>
        <% if(com != null){
            for(i=0;i<com.length;i++){ %>
                    <%= com[i]%><br>
                        <% }} %>
        
    </p><br>
  </div>
  <div class="w3-container">
    <h5 class="w3-opacity"><b>Date of Creation</b></h5>
    <p><%= kk[0].created_at%></p><br>
  </div>

 </div>
  <!-- End Right Column -->
  </div>

<!-- End Grid -->


<!-- End Page Container -->

<footer class="w3-container w3-teal w3-center w3-margin-top">

</footer>

</body>
