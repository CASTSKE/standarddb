				<%var result1 = forticount.find(item1 => item1.cweid.replace("CWE ID ","") === id);%>
							<%if(result || result1){%>		
					<td>
						<b><%=cweid[i].cwe_id %></b>
											
					</td>
					<td class="num">
					<% if (result){ %>
						<a href="/checkid?name=<%=cweid[i].cwe_id%>">			
        <%= result.count %></a>
		<%}%>
    
		</td>
					<td class="num">
						
											</td>
					<td class="num">
													
											</td>
					<td class="num">
						<%if(result1){%>	
						
						 <a href="/fortis?id=<%=result1.cweid%>">						
					<%= result1.count %></a>
		<%}%>
											</td>					
					
				</tr>
				 <%}%>
				<%}%>
			</table>
  
<!--
