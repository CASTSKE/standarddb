					</th>
				</tr></thead>
				
			<% for(var i=0; i<cweid.length; i++) {%>
				<tr onMouseOver="this.style.background='#F5F4B8'" onMouseOut="this.style.background=''">
				<%var id=cweid[i].cwe_id%>	
